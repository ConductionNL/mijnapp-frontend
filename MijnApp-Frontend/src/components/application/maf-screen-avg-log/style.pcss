.Wrapper {
  width: 100%;
  max-width: var(--main-max-width);
  margin: 0 auto;
}
.SubHeading {
  font-size: var(--main-font-subheader-size);
  color: var(--palette-secondary-main);
  font-weight: 500;
}
.Description {
  font-size: var(--main-font-subheader-size);
  color: var(--palette-primary-main);
  font-style: italic;
}
.AvgLogDataPoint {
  font-family: var(--typography-fontFamily);
  padding: 10px 0 13px 0;
  border-bottom: 1px solid #EFF1F3;
  position: relative;
}
.AvgLogDataPointLabel {
  opacity: 0.4;
  font-size: var(--main-font-subheader-size);
  font-weight: 500;
  font-style: normal;
  font-stretch: normal;
  line-height: 1.44;
  letter-spacing: -0.3px;
  text-align: left;
  color: var(--main-header-text-color);
}
.AvgLogDataPointValue {
  height: 23px;
  font-size: var(--main-font-text-size);
  font-weight: 600;
  font-style: normal;
  font-stretch: normal;
  line-height: 1.28;
  letter-spacing: -0.4px;
  text-align: left;
  color: var(--main-header-text-color);
}
.AvgLogDataPointSubValue {
  height: 23px;
  font-size: var(--main-font-medium-text-size);
  font-style: normal;
  font-stretch: normal;
  line-height: 1.28;
  letter-spacing: -0.4px;
  text-align: left;
  color: var(--main-header-text-color);
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.AvgLogDataPointSubValueKey {
  font-weight: 600;
}
.AvgLogDataPointSubValueValue {
  font-weight: 500;
}

.AvgLogDataPointSubSubValue {
  height: 23px;
  font-size: var(--main-font-small-text-size);
  font-style: normal;
  font-stretch: normal;
  line-height: 1.28;
  letter-spacing: -0.4px;
  text-align: left;
  color: var(--main-header-text-color);
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.AvgLogDataPointSubSubValueKey {
  font-weight: 500;
  margin-left: 16px;
}
.AvgLogDataPointSubSubValueValue {
  font-weight: 400;
}
.Logo {
  height: 30px;
  width: 30px;
  background-size: contain;
  background-repeat: no-repeat;
  background-position: center center;
  position: absolute;
  top: 2px;
  right: 0;
}
