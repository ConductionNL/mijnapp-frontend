.Wrapper {
  width: 100%;
  float: left;
}
.Inner {
  width: calc(100% - 32px);
  padding: 0 16px;
}
.ButtonContainer {
  width: 100%;
  height: 62px;
  max-width: var(--main-max-width);
  padding: 16px 0;
  box-sizing: border-box;
}
.BackButton {
  display: inline-block;
  opacity: 1;
  transform: scale(0.7, 1.2);
}
